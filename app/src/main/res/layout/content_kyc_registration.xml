        <EditText
            android:id="@+id/ethouses"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/houses1"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:ems="10"
            android:inputType="text" />

        <TextView
            android:id="@+id/furniture1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ethouses"
            android:layout_marginLeft="15dp"
            android:text="Furniture:" />

        <EditText
            android:id="@+id/etfurniture"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/furniture1"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:ems="10"
            android:inputType="text" />

        <TextView
            android:id="@+id/vehicle1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/etfurniture"
            android:layout_marginLeft="15dp"
            android:text="Vehicle(s):" />

        <EditText
            android:id="@+id/etvehicle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/vehicle1"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:ems="10"
            android:inputType="text" />

        <TextView
            android:id="@+id/land1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/etvehicle"
            android:layout_marginLeft="15dp"
            android:text="Land:" />

        <EditText
            android:id="@+id/etland"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/land1"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:ems="10"
            android:inputType="text" />


        <Button
            android:id="@+id/kycpicbtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/etland"
            android:background="#ff8c00"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="15dp"
            android:textColor="#322a29"
            android:text="Attach" />

        <ImageView
            android:id="@+id/kycpic"
            android:layout_width="60dp"
            android:layout_below="@id/etland"
            android:layout_toRightOf="@+id/kycpicbtn"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="20dp"
            android:layout_height="60dp"
            app:srcCompat="@mipmap/ic_launcher"/>



        <Button
            android:id="@+id/nextbtn"
            android:layout_below="@+id/kycpic"
            android:background="#322a29"
            android:layout_marginLeft="20dp"
            android:layout_marginBottom="20dp"
            android:layout_marginTop="20dp"
            android:textColor="#ff8c00"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="savekyc"
            android:text="SAVE DATA" />

        <Button
            android:id="@+id/kycnxtbtn"
            android:layout_below="@+id/nextbtn"
            android:background="#322a29"
            android:layout_marginLeft="20dp"
            android:layout_marginBottom="20dp"
            android:layout_marginTop="10dp"
            android:textColor="#ff8c00"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="questionnaire" />


    </RelativeLayout>
</ScrollView>
